<li role="presentation" class="{{ first ? 'active' : '' }}">
    <a href="#{{ index  }}" aria-controls="{{ index }}" role="tab" data-toggle="tab">
        <img class="img-circle" src="https://s3.amazonaws.com/uifaces/faces/twitter/dustinlamont/128.jpg" />
        <span class="quote"><i class="{{ (feedItem.icon is defined) ? feedItem.icon : options.default_icon }}"></i></span>
    </a>
</li>