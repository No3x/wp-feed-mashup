<!-- WPFM Shortcode BEGIN -->
<div class="container text-center">
    <div class="row">
        <div class="col-xs-12 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3" role="tabpanel">
            <div class="col-xs-4 col-sm-12">
                <!-- Nav tabs -->
                <ul class="nav nav-justified" id="nav-tabs" role="tablist">
                    {% for feed in feeds %}
                        {% set first = 'false' %}
                        {% include 'Feed.Tab.twig' with {'feed' : feed, 'first' : loop.first, 'options' : options} %}
                    {% else %}
                        No Feed yet
                    {% endfor %}
                </ul>
            </div>
            <div class="col-xs-8 col-sm-12">
                <!-- Tab panes -->
                <div class="tab-content" id="tabs-collapse">
                    {% for feed in feeds %}
                        {% include 'Feed.Pane.twig' with {'feed' : feed, 'first' : loop.first, 'options' : options} %}
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</div>
<!-- WPFM Shortcode END -->