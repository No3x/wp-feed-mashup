<!-- WPFM Shortcode BEGIN -->
<div class="container text-center">
    <div class="row">
        <div class="col-xs-12 col-sm-8 col-md-6" role="tabpanel">
            <div class="col-xs-4 col-sm-12">
                <!-- Nav tabs -->
                <ul class="nav nav-justified" id="nav-tabs" role="tablist">
                    {% for feedItem in feedItems %}
                        {% include 'Feed.Tab.twig' with {'feedItem' : feedItem, 'index' : loop.index, 'first' : loop.first, 'options' : options} %}
                    {% else %}
                        No Feeds yet
                    {% endfor %}
                </ul>
            </div>
            <div class="col-xs-8 col-sm-12">
                <!-- Tab panes -->
                <div class="tab-content" id="tabs-collapse">
                    {% for feedItem in feedItems %}
                        {% include 'Feed.Pane.twig' with {'feedItem' : feedItem, 'index' : loop.index, 'first' : loop.first, 'options' : options} %}
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</div>
<!-- WPFM Shortcode END -->